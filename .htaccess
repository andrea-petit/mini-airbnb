Options -Indexes

RewriteEngine On


RewriteCond %{HTTP_REFERER} ^$
RewriteRule ^(config|models|controllers|actions) - [F,L]


RedirectMatch 301 ^/airbnb?$ /airbnb/views/login.php

ErrorDocument 403 /airbnb/public/templates/403.php

ErrorDocument 404 /airbnb/public/templates/404.php

ErrorDocument 500 /airbnb/public/templates/500.php



